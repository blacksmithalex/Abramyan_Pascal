program Case1;
var
    x: Integer;
begin
    Readln(x);
    Case x of
        1: Writeln('понедельник');
        2: Writeln('вторник');
        3: Writeln('среда');
        4: Writeln('четверг');
        5: Writeln('пятница');
        6: Writeln('суббота');
        7: Writeln('воскресенье');
    end;
end.
